pipeline {

    // Run on any agent 
    agent any 
	
		  
	stages {
        stage('Sonar')	{
	
	steps
	{
			script {
                    
                    withSonarQubeEnv('sonar_scanner') {
						
						
						
		sh '''$SONAR_SCANNER_TOOL \
		-Dsonar.pullrequest.base=main -Dsonar.pullrequest.key=18 \
		-Dsonar.pullrequest.branch=sonarTest \
		-Dsonar.pullrequest.branch=sonarTest \
		-Dsonar.projectKey=Chaitanya-enterprise_SonarTest \
		-Dsonar.sources=. \
		-Dsonar.projectVersion=1.0''' \
		-Dsonar.pullrequest.provider=github \
		-Dsonar.pullrequest.github.repository=Chaitanya-Enterprise/SonarTest \
		-Dsonar.organization=Chaintaya-Enterprise \
                    }
			
	}	}		
}
}
}


